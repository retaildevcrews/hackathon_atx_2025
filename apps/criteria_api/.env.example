# CosmosDB connection settings
COSMOSDB_ENDPOINT=your-cosmosdb-endpoint
COSMOSDB_KEY=your-cosmosdb-key
COSMOSDB_DATABASE=criteria_db
COSMOSDB_CONTAINER=criteria

# Azure Storage settings (used by candidate material uploads)
# Connection string for Azure Storage Account (preferred)
AZURE_STORAGE_CONNECTION_STRING=
# Legacy/alternate env var name (kept for compatibility)
STORAGE_CONN_STRING=
# Container to store candidate materials (default: raw-docs)
CANDIDATE_MATERIALS_CONTAINER=raw-docs
